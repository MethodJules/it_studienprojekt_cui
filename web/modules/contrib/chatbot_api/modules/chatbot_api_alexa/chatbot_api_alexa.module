<?php

/**
 * @file
 * Contains chatbot_api_alexa.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function chatbot_api_alexa_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the chatbot_api_alexa module.
    case 'help.page.chatbot_api_alexa':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Alexa driver for Chatbot API') . '</p>';
      return $output;

    default:
  }
}
